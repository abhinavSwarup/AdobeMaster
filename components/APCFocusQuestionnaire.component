            <apex:outputPanel id="inputFile" layout="block" styleClass="row form-group">
                <div class="col-md-12" ex="{!mainCtrl.atchmt.Id != null}">
                    <apex:inputFile value="{!mainCtrl.atchmt.Body}" filename="{!mainCtrl.atchmt.Name}" filesize="{!mainCtrl.atchmt.BodyLength}"
                            onchange="validateFile(this);" id="atchmt"/>
                </div>
                <span style="display:none;" class="help-block parsley-required" id="atchmtReqDoc">
                    {!$Label.OnbrdGenInputErrorMsg}
                </span>
            </apex:outputPanel>
        </div>
    </apex:outputPanel>
    <script type="text/javascript">
        function validateFile(itm) {
            var msg;
            if (!itm.files || !itm.files.length) {
                var jParDiv = jQuery(itm).parent();
                if (jParDiv.attr('ex') == "false") {
                    if ('{!mainCtrl.eLink.Retail_Specialization__c}' == 'Online Retail') {
                        msg = '{!$Label.NAOnlineRetailAttachementRequired}';
                    } else if ('{!mainCtrl.eLink.Retail_Specialization__c}' == 'Digital Imaging/ProVideo') {
                        msg = '{!$Label.NADI_ProVAttachmentRequired}';
                    }
                }
            } else if (!itm.files[0].name || !itm.files[0].size) {
                msg = 'Invalid document';
            } else if (itm.files[0].name.length > 80) {
                msg = '{!$Label.SplznNameExceededSizeMsg}';
            } else if (itm.files[0].size > 5000000) {
                msg = '{!$Label.SplznRequiredExceededSizeMsg}';
            }
            if (msg) {
                jQuery('#atchmtReqDoc').text(msg).show();
                jQuery(itm).val('');
                return false;
            } else {
                jQuery('#atchmtReqDoc').hide();
                return true;
            }
        }
    </script>
